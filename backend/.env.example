# MindCanvas Environment Configuration
# Copy this file to .env and fill in your actual values

# OpenAI Configuration (required for LLM processing and chat)
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4o-mini

# Groq Configuration (optional - for faster inference fallback)
GROQ_API_KEY=your_groq_api_key_here

# Supabase Configuration (required for vector database)
SUPABASE_URL=your_supabase_project_url
SUPABASE_KEY=your_supabase_anon_key

# Application Settings
BACKEND_PORT=8090
FRONTEND_URL=http://localhost:3030
